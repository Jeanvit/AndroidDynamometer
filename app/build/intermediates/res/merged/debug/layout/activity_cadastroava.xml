<?xml version="1.0" encoding="utf-8"?>
<android.gesture.GestureOverlayView xmlns:android="http://schemas.android.com/apk/res/android"
android:id="@+id/gesturesAva"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:eventsInterceptionEnabled="true"
android:gestureStrokeType="multiple"
android:orientation="vertical" >

<AbsoluteLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:layout_width="374dp"
        android:layout_height="397dp"
        android:layout_x="435dp"
        android:layout_y="127dp" >

        <EditText
            android:id="@+id/edtAvatel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textView2"
            android:layout_toRightOf="@+id/TextView03"
            android:ems="10" />

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/edtAvatel"
            android:layout_alignParentLeft="true"
            android:text="Tel:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/TextView04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/edtAvaEmail"
            android:layout_alignParentLeft="true"
            android:text="Email:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/edtAvaEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/edtAvatel"
            android:layout_marginTop="22dp"
            android:layout_toRightOf="@+id/TextView04"
            android:ems="10" >

            <requestFocus />
        </EditText>

        <TextView
            android:id="@+id/TextView09"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Acesso:"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:text="Contato:"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp" />

        <EditText
            android:id="@+id/edtAvaSenha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TextView09"
            android:layout_toRightOf="@+id/TextView10"
            android:ems="10"
            android:inputType="textPassword" />

        <TextView
            android:id="@+id/TextView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/edtAvaConfSenha"
            android:text="Senha:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/edtAvaConfSenha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/edtAvaSenha"
            android:layout_marginTop="13dp"
            android:layout_toRightOf="@+id/TextView11"
            android:ems="10"
            android:inputType="textPassword" />

        <TextView
            android:id="@+id/TextView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/edtAvaConfSenha"
            android:text="Conf. Senha"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/edtInstituicao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/edtAvaEmail"
            android:layout_marginTop="11dp"
            android:layout_toRightOf="@+id/TextView12"
            android:ems="10" />

        <TextView
            android:id="@+id/TextView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/edtInstituicao"
            android:layout_alignParentLeft="true"
            android:text="Instituição:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="409dp"
        android:layout_height="392dp"
        android:layout_x="815dp"
        android:layout_y="128dp" >

        <TextView
            android:id="@+id/TextView06"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Informações adicionais:"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp" />

        <EditText
            android:id="@+id/edtAvaObservacao"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TextView08"
            android:layout_marginBottom="30dp"
            android:ems="10"
            android:inputType="textMultiLine" />

        <TextView
            android:id="@+id/TextView07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/TextView06"
            android:layout_centerHorizontal="true"
            android:text="Informações Profissionais"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/edtAvaAnotacao"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/TextView08"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TextView07"
            android:ems="10"
            android:inputType="textMultiLine" />

        <TextView
            android:id="@+id/TextView08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="Observações"
            android:textAppearance="?android:attr/textAppearanceMedium" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="374dp"
        android:layout_height="405dp"
        android:layout_x="8dp"
        android:layout_y="130dp" >

        <EditText
            android:id="@+id/edtAvaNome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/textView4"
            android:layout_below="@+id/textView4"
            android:layout_toRightOf="@+id/TextView01"
            android:ems="10" />

        <EditText
            android:id="@+id/edtAvaCidade"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView6"
            android:layout_alignRight="@+id/edtAvaNasc"
            android:layout_toRightOf="@+id/textView6"
            android:ems="10" />

        <EditText
            android:id="@+id/edtAvaEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/edtAvaNasc"
            android:layout_alignRight="@+id/edtAvaCidade"
            android:layout_below="@+id/textView6"
            android:layout_marginTop="21dp"
            android:ems="10" />

        <Spinner
            android:id="@+id/spnAvaSexo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/edtAvaNome"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/edtAvaNome"
            android:layout_marginTop="24dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/spnAvaSexo"
            android:layout_alignRight="@+id/TextView02"
            android:text="Sexo:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/edtAvaNasc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/edtAvaRG"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/edtAvaRG"
            android:layout_marginTop="22dp"
            android:ems="10"
            android:inputType="date" />

        <EditText
            android:id="@+id/edtAvaRG"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/spnAvaSexo"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:ems="10"
            android:inputType="number" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/edtAvaNasc"
            android:layout_alignRight="@+id/TextView02"
            android:text="RG:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/edtAvaNasc"
            android:layout_alignBottom="@+id/edtAvaNasc"
            android:layout_toLeftOf="@+id/edtAvaNasc"
            android:text="Nasc:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView5"
            android:layout_below="@+id/edtAvaNasc"
            android:layout_marginTop="36dp"
            android:text="Cidade/UF:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/TextView05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/edtAvaEnd"
            android:layout_alignLeft="@+id/textView6"
            android:text="End:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/edtAvaNome"
            android:layout_alignParentLeft="true"
            android:text="Nome:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:text=" Informações básicas:"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp" />

    </RelativeLayout>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="110dp"
        android:layout_y="29dp"
        android:text="Cadastro de Avaliadores"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="60sp" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_x="23dp"
        android:layout_y="24dp"
        android:src="@drawable/cadavaliadores" />

    <ImageButton
        android:id="@+id/btnAvaExames"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="815dp"
        android:layout_y="549dp"
        android:src="@drawable/btnexames" />

    <ImageButton
        android:id="@+id/btnAvaSalva"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="700dp"
        android:layout_y="550dp"
        android:src="@drawable/salvar" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="179dp"
        android:layout_x="283dp"
        android:layout_y="579dp"
        android:orientation="horizontal" >
    </LinearLayout>

    <ImageButton
        android:id="@+id/btnAvaExcluir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="587dp"
        android:layout_y="550dp"
        android:src="@drawable/deletar" />

    <ImageButton
        android:id="@+id/btnAvaEditar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="474dp"
        android:layout_y="550dp"
        android:src="@drawable/editar" />

    <ImageButton
        android:id="@+id/btnAvaPesquisa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="359dp"
        android:layout_y="550dp"
        android:src="@drawable/procurar" />

    <ImageView
        android:id="@+id/ImageView02"
        android:layout_width="504dp"
        android:layout_height="485dp"
        android:layout_x="954dp"
        android:layout_y="328dp"
        android:src="@drawable/fundo" />

</AbsoluteLayout>
</android.gesture.GestureOverlayView>