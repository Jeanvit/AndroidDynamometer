<?xml version="1.0" encoding="utf-8"?>
<android.gesture.GestureOverlayView xmlns:android="http://schemas.android.com/apk/res/android"
android:id="@+id/gestures"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:eventsInterceptionEnabled="true"
android:gestureStrokeType="multiple"
android:orientation="vertical" >

<AbsoluteLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    

    <RelativeLayout
        android:layout_width="374dp"
        android:layout_height="397dp"
        android:layout_x="435dp"
        android:layout_y="127dp" >

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="Contato:"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp" />

        <EditText
            android:id="@+id/EditText01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textView2"
            android:layout_toRightOf="@+id/TextView03"
            android:ems="10" />
		
        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/EditText01"
            android:layout_alignParentLeft="true"
            android:text="Tel:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/TextView04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/editText2"
            android:layout_alignParentLeft="true"
            android:text="Email:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/editText2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/EditText01"
            android:layout_marginTop="22dp"
            android:layout_toRightOf="@+id/TextView04"
            android:ems="10" >

            <requestFocus />
        </EditText>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="409dp"
        android:layout_height="392dp"
        android:layout_x="815dp"
        android:layout_y="128dp" >

        <TextView
            android:id="@+id/TextView06"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Informações adicionais:"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp" />

        <EditText
            android:id="@+id/edtObservacao"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TextView08"
            android:layout_marginBottom="30dp"
            android:ems="10"
            android:inputType="textMultiLine" />

        <TextView
            android:id="@+id/TextView07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/TextView06"
            android:layout_centerHorizontal="true"
            android:text="Anotações:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/TextView08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView07"
            android:layout_centerVertical="true"
            android:text="Observações"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/edtAnotacao"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/TextView08"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TextView07"
            android:ems="10"
            android:inputType="textMultiLine" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="374dp"
        android:layout_height="405dp"
        android:layout_x="8dp"
        android:layout_y="130dp" >

        <EditText
            android:id="@+id/editText6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/textView4"
            android:layout_below="@+id/textView4"
            android:layout_toRightOf="@+id/TextView01"
            android:ems="10" />

        <EditText
            android:id="@+id/editText5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView6"
            android:layout_alignRight="@+id/editText4"
            android:layout_toRightOf="@+id/textView6"
            android:ems="10" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/editText4"
            android:layout_alignRight="@+id/editText5"
            android:layout_below="@+id/textView6"
            android:layout_marginTop="21dp"
            android:ems="10" />

        <Spinner
            android:id="@+id/spnSexo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/editText6"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/editText6"
            android:layout_marginTop="24dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/spnSexo"
            android:layout_alignRight="@+id/TextView02"
            android:text="Sexo:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/editText4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/editText7"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/editText7"
            android:layout_marginTop="22dp"
            android:ems="10"
            android:inputType="date" />

        <EditText
            android:id="@+id/editText7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/spnSexo"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:ems="10"
            android:inputType="number" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/editText4"
            android:layout_alignRight="@+id/TextView02"
            android:text="RG:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/editText4"
            android:layout_alignBottom="@+id/editText4"
            android:layout_toLeftOf="@+id/editText4"
            android:text="Nasc:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView5"
            android:layout_below="@+id/editText4"
            android:layout_marginTop="36dp"
            android:text="Cidade/UF:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/TextView05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/editText3"
            android:layout_alignLeft="@+id/textView6"
            android:text="End:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/editText6"
            android:layout_alignParentLeft="true"
            android:text="Nome:"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:text=" Informações básicas:"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp" />

    </RelativeLayout>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="110dp"
        android:layout_y="29dp"
        android:text="Cadastro"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="60sp" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_x="23dp"
        android:layout_y="24dp"
        android:src="@drawable/cadastro" />

    <ImageButton
        android:id="@+id/btnExames"
        android:layout_width="130dp"
        android:layout_height="145dp"
        android:layout_x="821dp"
        android:layout_y="510dp"
        android:src="@drawable/exames" />

    <ImageButton
        android:id="@+id/btnSalva"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="700dp"
        android:layout_y="550dp"
        android:src="@drawable/salvar" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="179dp"
        android:layout_x="283dp"
        android:layout_y="579dp"
        android:orientation="horizontal" >
    </LinearLayout>

    <ImageButton
        android:id="@+id/btnExcluir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="587dp"
        android:layout_y="550dp"
        android:src="@drawable/deletar" />

    <ImageButton
        android:id="@+id/btnEditar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="474dp"
        android:layout_y="550dp"
        android:src="@drawable/editar" />

    <ImageButton
        android:id="@+id/Button01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="359dp"
        android:layout_y="550dp"
        android:src="@drawable/procurar" />

    <ImageView
        android:id="@+id/ImageView02"
        android:layout_width="504dp"
        android:layout_height="485dp"
        android:layout_x="954dp"
        android:layout_y="328dp"
        android:src="@drawable/fundo" />

</AbsoluteLayout>
</android.gesture.GestureOverlayView>